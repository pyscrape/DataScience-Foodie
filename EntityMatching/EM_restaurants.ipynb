   "outputs": [],
   "source": [
    "## Instantiate blackbox blocker\n",
    "bb = em.BlackBoxBlocker()\n",
    "\n",
    "## Set the black box function\n",
    "bb.set_black_box_function(zipcode_match)"
   ]
