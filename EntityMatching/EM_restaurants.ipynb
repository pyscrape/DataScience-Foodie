    "collapsed": true,
    "deletable": true,
    "editable": true
   },
   "outputs": [],
   "source": [
    "## Create a set of ML Matchers\n",
    "dt = em.DTMatcher()\n",
    "rf = em.RFMatcher()\n",
    "nb = em.NBMatcher()\n",
    "logreg = em.LogRegMatcher()\n",
    "linreg = em.LinRegMatcher()\n",
    "svm = em.SVMMatcher()"
