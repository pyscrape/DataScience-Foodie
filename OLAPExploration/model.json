{
    "dimensions": [
        {
         "name":"location",
         "levels": [
                {
                    "name":"street",
                    "label":"Street",
                    "attributes": ["street"]
                },
                {
                    "name":"city",
                    "label":"City",
                    "attributes": ["city"]
                },
                {
                    "name":"state",
                    "label":"State",
                    "attributes": ["state"]
                },
                {
                    "name":"zipcode",
                    "label":"Zipcode",
                    "attributes": ["zipcode"]
                }
            ],
            "hierarchies": [
                {
                    "name": "bystate",
                    "label": "bystate",
                    "levels": ["state"]
                }
            ]
        },
        {
          "name":"name",
          "label":"Restaurant",
          "attributes":["name"]
        }
    ],
    "cubes": [
        {
            "name": "restaurant_details",
            "dimensions": ["location","name"],
            "measures": [
                {"name":"ratingValue", "label":"rating_average"},
                {"name":"price_range", "label":"price_average"},
                {"name":"number_of_reviews", "label":"total_number_of_reviews"}
            ],
            "aggregates": [
                    {
                        "name": "rating_average",
                        "function": "avg",
                        "measure":"ratingValue"
                    },
                    {
                        "name": "price_average",
                        "function": "avg",
                        "measure": "price_range"
                    },
                    {
                    "name":"total_number_of_reviews",
                    "function": "sum",
                    "measure": "number_of_reviews"
                    }
             ],
            "mappings": {
                          "location.street": "street",
                          "location.city": "city",
			              "location.state": "state",
                          "location.zipcode": "zipcode"
            }

        }
    ]
}
